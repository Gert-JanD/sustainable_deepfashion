{
  "copy_instruction": "simple_conv2d.json",
  "model": {
    "factory": "src.models.simple_conv2d_triplet_model_factory.SimpleConv2DTripletModelFactory",
    "kwargs": {
      "embedding_size": 32
    }
  },
  "loss": {
    "loss": "tensorflow_addons.losses.TripletSemiHardLoss",
    "kwargs": {
      "margin": 0.75,
      "distance_metric": "L2"
    }
  },
  "callbacks": {
    "src.models.callbacks.CyclicLR": {
      "base_lr": 1e-5,
      "max_lr": 1e-3,
      "step_size": 5184,
      "mode": "exp_range"
    }
  }
}
